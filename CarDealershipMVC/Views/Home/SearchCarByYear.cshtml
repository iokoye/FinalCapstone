@model IEnumerable<CarDealershipMVC.Models.CarSearchViewModel>

@{
    ViewData["Title"] = "SearchCarByYear";
}

<h1>SearchCarByYear</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>           
        </tr>
}
    </tbody>
</table>
